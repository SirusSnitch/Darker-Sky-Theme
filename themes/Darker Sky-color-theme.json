	"tokenColors": [
		{
			"scope": [
				"comment",
				"punctuation.definition.comment"
		],
			"settings": {
				"foreground": "#808392"
			}
		},
		{
			"scope": [
				"constant",
				"entity.name.constant",
				"variable.other.constant",
				"variable.language",
			],
			"settings": {
				"foreground": "#ffe553"
			}
		},
		{
			"scope": [
				"punctuation.definition.decorator",
				"entity.name.function.decorator"
			],
			"settings": {
				"foreground": "#FFB114"
			}
		},
		{
			"scope": [
				"entity",
				"entity.name",
				"entity.name.function",
				"entity.name.method",
				"meta.class entity.name.function"
			],
			"settings": {
				"foreground": "#61afef"
			}
		},
		{
			"scope": [
				"variable",
				"variable.parameter",
				"entity.name.variable",
				"meta.object-literal.key",
				"support.variable",
				"keyword.control.import"
			],
			"settings": {
				"foreground": "#5FCED1"
			}
		},
		{
			"scope": [
				"keyword",
				"storage",
				"meta.var.expr storage.type",
				"storage.type.annotation"
			],
			"settings": {
				"foreground": "#EC47AD"
			}
		},
		{
			"scope": ["keyword.operator"],
			"settings": {
				"foreground": "#ec47ad"
			}
		},
		{
			"scope": ["keyword.operator.new"],
			"settings": {
				"foreground": "#01d648"
			}
		},
		{
			"scope": [
				"keyword.operator.expression",
				"keyword.operator.arithmetic",
				"keyword.operator.comparison",
				"keyword.operator.decrement",
				"keyword.operator.increment"
			],
			"settings": {
				"foreground": "#EC47AD"
			}
		},
		{
			"scope": [
				"keyword.operator.relational",
				"keyword.operator.logical"
			],
			"settings": {
				"foreground": "#ffe553"
			}
		},
		{
			"scope": ["string"],
			"settings": {
				"foreground": "#5966cc"
			}
		},
		{
			"scope": [
				"punctuation",
				"meta.tag entity.name.tag",
				"meta.brace",
				"meta.tag",
				"meta.tag.entity.name.tag"
			],
			"settings": {
				"foreground": "#5FCED1"
			}
		},
		{
			"scope": [
				"support",
				"support.type",
				"support.class",
				"support.orther.namespace",
				"support.function"
			],
			"settings": {
				"foreground": "#5966cc"
			}
		},
		{
			"scope": [
				"meta.import entity.name",
				"meta.import string"
			],
			"settings": {
				"foreground": "#01d648"
			}
		},
		{
			"scope": ["meta.tag.sgml.doctype.html"],
			"settings": {
				"foreground": "#8c97ea"
			}
		},
		{
			"scope": ["meta.selector"],
			"settings": {
				"foreground": "#ec47ad"
			}
		},
		{
			"scope": ["entity.name.tag.css"],
			"settings": {
				"foreground": "#5966cc"
			}
		},
		{
			"scope": ["meta.selector entity.name.tag"],
			"settings": {
				"foreground": "#5966cc"
			}
		},
		{
			"scope": ["support.type.property-name"],
			"settings": {
				"foreground": "#8c97ea"
			}
		},
		{
			"scope": ["meta.property-name entity.name.tag"],
			"settings": {
				"foreground": "#8c97ea"
			}
		},
		{
			"scope": [
				"meta.tag.sgml.doctype.html",
				"meta.tag.sgml.doctype.html"
			],
			"settings": {
				"foreground": "#EC47AD"
			}
		},
		{
			"scope": ["meta.attribute"],
			"settings": {
				"foreground": "#5966cc"
			}
		},
		{
			"scope": ["markup.bold"],
			"settings": {
				"foreground": "#8c97ea"
			}
		},
		{
			"scope": ["markup.italic"],
			"settings": {
				"foreground": "#ec47ad"
			}
		},
		{
			"scope": ["markup.underline"],
			"settings": {
				"foreground": "#ffe553"
			}
		},
		{
			"scope": ["markup.strikethrough"],
			"settings": {
				"foreground": "#EC47AD"
			}
		},
		{
			"scope": ["markup.quote"],
			"settings": {
				"foreground": "#01d648"
			}
		}
	]
